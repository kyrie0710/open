    <script>
        const tooltipTitleCallback = (tooltipItems) => {
            const item = tooltipItems[0];
            let label = item.chart.data.labels[item.dataIndex];
            if (Array.isArray(label)) {
                return label.join(' ');
            }
            return label;
        };

        const wrapLabel = (label) => {
            const maxLength = 16;
            if (label.length <= maxLength) return label;
            
            const words = label.split(' ');
            const lines = [];
            let currentLine = '';

            words.forEach(word => {
                if ((currentLine + word).length > maxLength) {
                    lines.push(currentLine.trim());
                    currentLine = '';
                }
                currentLine += word + ' ';
            });
            lines.push(currentLine.trim());
            return lines.filter(line => line.length > 0);
        };

        const commonChartOptions = {
            responsive: true,
            maintainAspectRatio: false,
            plugins: {
                legend: {
                    position: 'top',
                    labels: {
                        font: {
                            family: "'Noto Sans KR', sans-serif",
                            size: 12
                        }
                    }
                },
                tooltip: {
                    callbacks: {
                        title: tooltipTitleCallback
                    },
                    bodyFont: {
                        family: "'Noto Sans KR', sans-serif"
                    },
                    titleFont: {
                        family: "'Noto Sans KR', sans-serif"
                    }
                }
            },
            scales: {
                x: {
                    ticks: {
                        font: {
                           family: "'Noto Sans KR', sans-serif"
                        }
                    }
                },
                y: {
                    ticks: {
                        font: {
                           family: "'Noto Sans KR', sans-serif"
                        }
                    }
                }
            }
        };

        const energeticPlayfulPalette = {
            red: '#FF6B6B',
            yellow: '#FFD166',
            green: '#06D6A0',
            blue: '#118AB2',
            darkBlue: '#073B4C'
        };
        
        const aprMarketCapCtx = document.getElementById('aprMarketCapChart').getContext('2d');
        new Chart(aprMarketCapCtx, {
            type: 'line',
            data: {
                labels: ['1ì›”', '3ì›”', '5ì›”', '7ì›”', '8ì›”'],
                datasets: [{
                    label: 'APR ì‹œê°€ì´ì•¡ (ì¡° ì›)',
                    data: [1.8, 3.5, 5.2, 7.1, 8.0],
                    borderColor: energeticPlayfulPalette.red,
                    backgroundColor: 'rgba(255, 107, 107, 0.2)',
                    fill: true,
                    tension: 0.4
                }]
            },
            options: {
                ...commonChartOptions,
                scales: {
                    y: {
                        beginAtZero: true,
                        title: {
                            display: true,
                            text: 'ì¡° ì›'
                        }
                    }
                }
            }
        });

        const odmRevenueCtx = document.getElementById('odmRevenueChart').getContext('2d');
        new Chart(odmRevenueCtx, {
            type: 'bar',
            data: {
                labels: ['ì½”ìŠ¤ë§¥ìŠ¤', 'í•œêµ­ì½œë§ˆ'],
                datasets: [{
                    label: '2ë¶„ê¸° ë§¤ì¶œ (ì–µ ì›)',
                    data: [6236, 5337],
                    backgroundColor: [energeticPlayfulPalette.blue, energeticPlayfulPalette.green],
                    borderColor: [energeticPlayfulPalette.darkBlue, energeticPlayfulPalette.darkBlue],
                    borderWidth: 1
                }, {
                    label: '2ë¶„ê¸° ì˜ì—…ì´ìµ (ì–µ ì›)',
                    data: [608, 410],
                    backgroundColor: [energeticPlayfulPalette.yellow, energeticPlayfulPalette.red],
                    borderColor: [energeticPlayfulPalette.darkBlue, energeticPlayfulPalette.darkBlue],
                    borderWidth: 1
                }]
            },
            options: {
                ...commonChartOptions,
                indexAxis: 'x',
                 scales: {
                    y: {
                        beginAtZero: true,
                        title: {
                            display: true,
                            text: 'ì–µ ì›'
                        }
                    }
                }
            }
        });

        const exportMarketCtx = document.getElementById('exportMarketChart').getContext('2d');
        new Chart(exportMarketCtx, {
